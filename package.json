{
  "name": "esbuild-ie-tpl",
  "version": "0.0.0",
  "description": "esbuild-ie-tpl",
  "main": "index.js",
  "scripts": {
    "start": "",
    "build": "npm-run-all --parallel build:*",
    "watch": "npm-run-all --parallel watch:*",
    "release": "npm-run-all --parallel release:* --serial build_js_ie",
    "build:js_ie": "node bin/build_js_ie.js",
    "build:js": "esbuild src/ts/app.ts --bundle --sourcemap --outfile=dist/app.js",
    "watch:js": "esbuild src/ts/app.ts --bundle --sourcemap --watch --outfile=dist/app.js",
    "release:js": "esbuild src/ts/app.ts --bundle --minify --sourcemap --outfile=dist/app.js",
    "build:css": "postcss src/css/app.css --output dist/app.css",
    "watch:css": "postcss src/css/app.css --output dist/app.css --watch",
    "release:css": "postcss --env production src/css/app.css --output dist/app.css",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "author": "",
  "license": "ISC",
  "browserslist": [
    "last 2 version",
    "> 1%",
    "ie 11"
  ],
  "devDependencies": {
    "@babel/core": "^7.14.3",
    "@babel/plugin-transform-runtime": "^7.14.3",
    "@babel/preset-env": "^7.14.2",
    "@babel/runtime-corejs3": "^7.14.0",
    "cssnano": "^5.0.4",
    "cssremedy": "0.1.0-beta.2",
    "esbuild": "^0.12.1",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.3.0",
    "postcss-cli": "^8.3.1",
    "postcss-import": "^14.0.2",
    "postcss-preset-env": "^6.7.0"
  }
}
